<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/margin_large"
    android:layout_marginRight="@dimen/margin_large"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.samir.andrew.myticket.views.activity.MyTickets"
    tools:showIn="@layout/activity_my_tickets">

    <com.malinskiy.superrecyclerview.SuperRecyclerView xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/rvMyTickets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_empty="@layout/xml_no_result_my_tickets"
        app:recyclerClipToPadding="false" />

</LinearLayout>
